{
  "name": "pagedao-hub",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:core": "yarn workspace @pagedao/core dev",
    "build:core": "yarn workspace @pagedao/core build",
    "dev:dashboard": "yarn workspace @pagedao/dashboard dev",
    "build:dashboard": "yarn workspace @pagedao/dashboard build",
    "dev": "concurrently \"yarn dev:core\" \"yarn dev:dashboard\"",
    "build": "yarn build:core && yarn build:dashboard"
  },
  "keywords": [
    "pagedao",
    "ethereum",
    "optimism",
    "base",
    "osmosis"
  ],
  "author": {
    "name": "PageDAO"
  },
  "license": "MIT",
  "description": "A comprehensive dashboard and API suite for the PageDAO ecosystem",
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
